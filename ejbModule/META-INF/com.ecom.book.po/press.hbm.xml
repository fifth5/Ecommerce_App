<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<hibernate-mapping package="com.ecom.book.po">
		<class name="Press" table="t_press">
			<id name="pressId" column="press_id">
				<generator class="uuid.hex"></generator>
			</id>
			<property name="pressName" column="press_name"></property>
			<property name="introduction" column="introduction"></property>
			<set name="books" table="t_book_press" lazy="false">
				<key column="press_id"></key>
				<many-to-many class="ProductBook" column="book_id"></many-to-many>
			</set>
		</class>
	</hibernate-mapping>
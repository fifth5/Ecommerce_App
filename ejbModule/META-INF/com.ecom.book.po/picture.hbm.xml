<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	<hibernate-mapping package="com.ecom.book.po">
		<class name="Picture" table="t_picture">
			<id name="pictureId" column="picture_id">
				<generator class="uuid.hex"></generator>
			</id>
			<property name="pictureUrl" column="picture_url" length="255"></property>
			<property name="pictureInfo" column="picture_info" length="1000"></property>
		</class>
	</hibernate-mapping>